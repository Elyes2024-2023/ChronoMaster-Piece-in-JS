# API URLs
VITE_API_URL=https://api.chronomasterpiece.com
VITE_SOCKET_URL=wss://socket.chronomasterpiece.com
VITE_WEATHER_API_URL=https://api.weatherapi.com/v1

# API Keys
VITE_WEATHER_API_KEY=your_weather_api_key_here

# Feature Flags
VITE_ENABLE_PREMIUM_FEATURES=true
VITE_ENABLE_SOCIAL_FEATURES=true

# Performance
VITE_MAX_RECONNECTION_ATTEMPTS=5
VITE_RECONNECTION_DELAY=2000
VITE_API_TIMEOUT=5000

# Analytics
VITE_ANALYTICS_ID=your_analytics_id_here

# Cache
VITE_CACHE_VERSION=v1
VITE_CACHE_DURATION=3600 